mmf <- function(A, B) {
ra <- nrow(A)
ca <- ncol(A)
rb <- nrow(B)
cb <- ncol(B)
if (ca == rb) {
result <- .Fortran("mat_mul_for",
A = as.double(A),
B = as.double(B),
AB = double(ra * cb),
RowA = as.integer(ra),
ColA = as.integer(ca),
RowB = as.integer(rb),
ColB = as.integer(cb),
RowAB = as.integer(ra),
ColAB = as.integer(cb)
)
mm <- matrix(result$AB, nrow = result$RowAB, byrow = F)
} else {
stop('non-conformable arguments')
}
return(list(Result = mm,
Dimension = c(result$RowAB, result$ColAB)
)
)
}
